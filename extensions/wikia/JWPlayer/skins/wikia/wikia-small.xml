<?xml version='1.0'?>
<skin version='1.1' name='Wikia' author='http://www.wikia.com/'>
	<components>
		<component name='controlbar'>
			<settings>
				<setting name='margin' value='15'/>
				<setting name='font' value='_sans'/>
				<setting name='fontsize' value='12'/>
				<setting name='fontcolor' value='0xFFFFFF'/>
				<setting name='fontstyle' value='normal'/>
				<setting name='fontweight' value='normal'/>
			</settings>
			<layout>
				<group position='left'>
					<button name='play'/>
				</group>
				<group position='right'>
					<button name='blank'/>
					<slider name='volume'/>
					<button name='fullscreen'/>
				</group>
			</layout>
			<elements>
				<element name='background' src='background.png'/>
				<element name='capLeft' src='capleft.png'/>
				<element name='capRight' src='capright.png'/>				
				<element name='playButton' src='play.png'/>
				<element name='playButtonOver' src='playover.png'/>
				<element name='pauseButton' src='pause.png'/>
				<element name='pauseButtonOver' src='pauseover.png'/>
				<element name='fullscreenButton' src='fullscreen.png'/>
				<element name='fullscreenButtonOver' src='fullscreenover.png'/>
				<element name='normalscreenButton' src='normalscreen.png'/>
				<element name='normalscreenButtonOver' src='normalscreenover.png'/>				
				<element name='volumeSliderRail' src='volumesliderrail.png'/>
				<element name='volumeSliderProgress' src='volumesliderprogress.png'/>
				<element name='volumeSliderThumb' src='volumesliderthumb.png'/>
				<element name='blankButton' src='backgroundtransparent.png'/>
				<element name='blankButtonOver' src='hdover.png'/>
			</elements>
		</component>
		<component name='hd'>
			<elements>
				<element name='controlbarIcon' src='HD_Icon_Normal.png'/>
			</elements>
		</component>
	</components>
</skin>